{
  "name": "comprehensive-farm-management-system",
  "version": "1.0.0",
  "description": "A comprehensive farm management system with animal health, biosecurity, inventory, IoT integration, and more",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "test": "jest",
    "lint": "eslint .",
    "migrate": "knex migrate:latest --knexfile server/knexfile.js",
    "seed": "knex seed:run --knexfile server/knexfile.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "knex": "^2.5.1",
    "pg": "^8.11.3",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.2",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.4",
    "axios": "^1.5.0",
    "crypto": "^1.0.1",
    "joi": "^17.9.2",
    "winston": "^3.10.0",
    "redis": "^4.6.7",
    "mqtt": "^5.0.5",
    "web3": "^4.1.1",
    "tensorflow": "^4.10.0",
    "@tensorflow/tfjs-node": "^4.10.0",
    "sharp": "^0.32.5",
    "pdf-lib": "^1.17.1",
    "exceljs": "^4.4.0",
    "qrcode": "^1.5.3",
    "uuid": "^9.0.0",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^2.4.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "eslint": "^8.47.0",
    "@babel/preset-env": "^7.22.9"
  },
  "keywords": [
    "farm-management",
    "agriculture",
    "livestock",
    "IoT",
    "blockchain",
    "AI",
    "biosecurity",
    "inventory"
  ],
  "author": "Farm Management System",
  "license": "MIT"
}